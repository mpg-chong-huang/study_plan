<h1> New Study Plan </h1>

<%= form_for @study_plan, url: new_study_plan_path, method: "GET" do |form| %>
  <div class="form-control">
    <%= form.label :start_date %>
    <%= form.text_field :start_date, type: "date" %>
  </div>
  <div class="form-control">
    <%= form.label :end_date %>
    <%= form.text_field :end_date, type: "date" %>
  </div>
  <div class="action">
    <%= form.submit "Generate Plan" %>
  </div>

<% end %>

<%= render partial: "form" %>